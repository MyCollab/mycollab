<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

    <context id="MySQL" targetRuntime="MyBatis3">

        <plugin type="com.mycollab.mybatis.plugin.MyCollabModelFilePlugin"/>
        <plugin type="org.mybatis.generator.plugins.FluentBuilderMethodsPlugin" />
        <plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin" />

        <jdbcConnection connectionURL="jdbc:mysql://localhost:3306/mycollab?verifyServerCertificate=false"
                        driverClass="com.mysql.cj.jdbc.Driver" password="mycollab123" userId="root"/>

        <javaTypeResolver>
            <property name="useJSR310Types" value="true"/>
        </javaTypeResolver>

        <javaModelGenerator targetPackage="com.mycollab.common.domain" targetProject="src/main/java">
            <property name="rootClass" value="com.mycollab.core.arguments.ValuedBean"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="." targetProject="src/main/resources/sqlMap/common"/>

        <javaClientGenerator targetPackage="com.mycollab.common.dao" targetProject="src/main/java" type="XMLMAPPER">
            <property name="exampleMethodVisibility" value="public"/>
            <property name="rootInterface" value="com.mycollab.db.persistence.ICrudGenericDAO"/>
        </javaClientGenerator>

        <table schema="mycollab" tableName="m_client" domainObjectName="Client"/>
        <table schema="mycollab" tableName="m_monitor_item" domainObjectName="MonitorItem"/>
        <table schema="mycollab" tableName="s_activitystream" domainObjectName="ActivityStream"/>
        <table schema="mycollab" tableName="m_comment" domainObjectName="Comment"/>
        <table schema="mycollab" tableName="s_customer_feedback" domainObjectName="CustomerFeedback"/>
        <table schema="mycollab" tableName="s_relay_email_notification" domainObjectName="RelayEmailNotification"/>
        <table schema="mycollab" tableName="m_options" domainObjectName="OptionVal"/>
        <table schema="mycollab" tableName="m_audit_log" domainObjectName="AuditLog"/>
        <table schema="mycollab" tableName="s_save_search_result" domainObjectName="SaveSearchResult"/>
        <table schema="mycollab" tableName="s_table_customize_view" domainObjectName="CustomViewStore"/>
        <table schema="mycollab" tableName="m_driveinfo" domainObjectName="DriveInfo"/>
    </context>

</generatorConfiguration>